{
  "expo": {
    "name": "Flocky",
    "slug": "fyp-flocky",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "android"
    ],
    "icon": "./assets/flocky-assets/flocky-icon.png",
    "splash": {
      "image": "./assets/flocky-assets/Flocky.png",
      "resizeMode": "contain",
      "backgroundColor": "#5188E3"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/flocky-assets/flocky-icon.png",
          "color": "#5188E3",
          "sounds": []
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/flocky-assets/flocky-icon.png",
        "backgroundColor": "#5188E3"
      },
      "package": "com.flocky.fypflocky",
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}